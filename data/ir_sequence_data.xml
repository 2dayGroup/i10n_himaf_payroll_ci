<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payroll_sequence" model="ir.sequence">
        <field name="name">Paie sequence</field>
        <field name="implementation">no_gap</field>
        <field name="code">salary.slip</field>
        <field name="prefix">PAIE/%(year)s/%(month)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

     <record id="employee_sequence" model="ir.sequence">
        <field name="name">Employee sequence</field>
        <field name="implementation">no_gap</field>
        <field name="code">hr.employee</field>
        <field name="prefix">E%(y)s-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
